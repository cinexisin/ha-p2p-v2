{
  "name": "Cinexis Remote Setup",
  "version": "0.1.0",
  "slug": "cinexis_remote",
  "description": "Applies Cinexis branding (dashboard + theme).",
  "arch": ["aarch64", "amd64", "armv7", "armhf", "i386"],
  "startup": "once",
  "boot": "auto",
  "init": false,
  "map": ["config:rw"],
  "options": {
    "home_label": "Cinexis Smart Home",
    "apply_dashboard": true,
    "apply_theme": true,
    "set_yaml_mode": true,
    "backup_existing_ui": true
  },
  "schema": {
    "home_label": "str",
    "apply_dashboard": "bool",
    "apply_theme": "bool",
    "set_yaml_mode": "bool",
    "backup_existing_ui": "bool"
  }
}
