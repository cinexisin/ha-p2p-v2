{
  "name": "Cinexis Remote FRPC",
  "version": "0.2.14",
  "slug": "cinexis_remote",
  "description": "Cinexis Remote Access: securely expose Home Assistant UI using Cinexis Cloud (FRP)\n\nLicensing:\n- license_key: required (e.g. CINX-DEMO-0001)\n- frps_addr/frps_port: optional (defaults to VPS IP + 7000)\n- ha_subdomain_suffix: optional (default .ha.cinexis.cloud)\n",
  "arch": [
    "amd64",
    "aarch64",
    "armv7"
  ],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "host_network": false,
  "ingress": false,
  "panel_icon": "mdi:cloud-lock",
  "options": {
    "frps_host": "cinexis.cloud",
    "frps_port": 7000,
    "pair_api": "https://pair.cinexis.cloud",
    "pair_code": "",
    "ha_scheme": "http",
    "ha_host": "homeassistant",
    "ha_port": 8123,
    "license_key": "",
    "frps_addr": "139.99.56.240",
    "ha_subdomain_suffix": ".ha.cinexis.cloud",
    "debug_print_secret": false
  },
  "schema": {
    "frps_host": "str",
    "frps_port": "int",
    "pair_api": "str",
    "pair_code": "str",
    "ha_scheme": "list(http|https)",
    "ha_host": "str",
    "ha_port": "int",
    "license_key": "str",
    "frps_addr": "str?",
    "ha_subdomain_suffix": "str?",
    "debug_print_secret": "bool?"
  }
}
