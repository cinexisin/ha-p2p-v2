{
  "name": "Cinexis Remote FRPC",
  "version": "0.2.3",
  "slug": "cinexis_remote",
  "description": "Cinexis Remote Access: securely expose Home Assistant UI using Cinexis Cloud (FRP)",
  "arch": ["amd64", "aarch64", "armv7"],

  "startup": "services",
  "boot": "auto",
  "init": false,

  "host_network": true,
  "ingress": false,

  "panel_icon": "mdi:cloud-lock",

  "map": ["config:rw", "share:rw", "ssl:rw"],

  "options": {
    "frps_host": "cinexis.cloud",
    "frps_port": 7000,
    "token": "",
    "home_id": "",
    "ha_scheme": "http",
    "ha_host": "127.0.0.1",
    "ha_port": 8123
  },

  "schema": {
    "frps_host": "str",
    "frps_port": "int",
    "token": "password",
    "home_id": "str",
    "ha_scheme": "list(http|https)",
    "ha_host": "str",
    "ha_port": "int"
  }
}
